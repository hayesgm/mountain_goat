<div id="container-main" class="mg-test-edit">
	<h1>Edit <%= @test.title.capitalize %></h1>
	<div class="container-content">
		<div class="edit-form">
			<% form_for :test, @test, :url => mg_test_url(:id => @test.id), :html => { :method => :put } do |f| %>
		 		<%= f.error_messages %>
		
				<%= render 'test_form', :f => f %>
			<% end %>
		</div>
	</div>
</div>