<div id="container-main" class="mg-goal-new">
	<h1>Create New Goal</h1>
	<div class="container-content">
		<div class="edit-form">
			<% form_for :goal, @goal, :url => mg_goals_url do |f| %>
				<%= f.error_messages %>
			
				<%= render :partial => 'mg/goals/goal_form', :locals => { :f => f } %>
			<% end %>
		</div>
	</div>
</div>